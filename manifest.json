{
  "name": "ALIBABA2",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "alibaba2",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.6.0.min.js", "getDataFromDOM.js"]
    }
  ],
  "permissions": ["storage"]
}